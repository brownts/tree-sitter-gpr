{
  "name": "tree-sitter-gpr",
  "version": "0.1.0",
  "description": "GPR grammar for node-tree-sitter",
  "main": "bindings/node",
  "scripts": {
    "generate": "tree-sitter generate",
    "highlight": "tree-sitter highlight",
    "test": "tree-sitter test",
    "gpr-test": "make -f test/gpr/Makefile"
  },
  "tree-sitter": [
    {
      "scope": "source.gpr",
      "file-types": [
        "gpr", "cgpr"
      ],
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ],
  "keywords": [
    "parser",
    "lexer"
  ],
  "author": "Troy Brown",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.7"
  }
}
